<view class="container">
    <view class="driving-bottom">
        <view class="time margin-top-sm">
            <text class="cuIcon-time lg text-orange margin-right-sm"></text>
            <text >{{elapsed}}</text>
        </view>
        <view class="fee">
            费用：{{fee}}元
        </view>
        <button class="cu-btn bg-red margin-top-sm lg">结束行程</button>
    </view>
    <map 
    id="mapId"
    class="map"
    latitude="{{latitude}}"
    longitude="{{longitude}}"
    scale="{{scale}}"
    show-location/>
</view>